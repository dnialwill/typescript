{
  "collectCoverage": true,
  "collectCoverageFrom": [
    "**/src/**/*.{js,jsx,ts,tsx}"
  ],
  "coverageReporters": [
    "html",
    "json"
  ],
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json",
    "node"
  ],
  "setupTestFrameworkScriptFile": "./test.setup.js",
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
  "testResultsProcessor": "./node_modules/jest-html-reporter",
  "transform": {
    "\\.(ts|tsx)$": "ts-jest"
  }
}